rate=16000
enabled=1

[audio_codec_1]
mime=speex
rate=8000
enabled=1

[audio_codec_2]
mime=PCMU
rate=8000
enabled=1

[audio_codec_3]
mime=GSM
rate=8000
enabled=1

[audio_codec_4]
mime=PCMA
rate=8000
enabled=1

[video_codec_0]
